<%= form_with model: @flat do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :rent %>
  <%= f.number_field :rent %>
  <%= f.label :pictures %>
  <%= f.number_field :pictures %>
  <%= f.submit %>
<% end %>
<%= link_to 'View all flats', flats_path %>
